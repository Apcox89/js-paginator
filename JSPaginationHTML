<div id="paginationJSHelper" style="width:90%; margin:0 auto;">
    <script src="~/PaginationJSHelper.js"></script>
      <script>
        let paginationDetails = PaginationHelper.createPaginationDetails(Model.Pagination.CurrentPage, Model.Pagination.TotalPages, Model.Pagination.TotalItems, Model.Pagination.PageSize);
        let getUrl = PaginationHelper.getUrl('@Model.BaseUrl', Model.Pagination.CurrentPage, Model.Pagination.PageSize);
        let paginationHtml = PaginationHelper.generateHtmlForPagination('@Model.BaseUrl', paginationDetails, Model.Pagination.CurrentPage, Model.Pagination.TotalPages, Model.Pagination.TotalItems, Model.Pagination.PageSize);
        document.write(paginationHtml);
    </script>
</div>